{
	"name": "strife.js",
	"version": "4.4.0",
	"description": "A Discord bot framework built around TypeScript support and ease of setup.",
	"keywords": [
		"discord",
		"discord-bot",
		"discord.js",
		"typescript"
	],
	"homepage": "https://discord.gg/WaEbDrXKxK",
	"bugs": {
		"url": "https://github.com/cobaltt7/strife.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cobaltt7/strife.js.git"
	},
	"license": "MIT",
	"author": {
		"name": "cobaltt7",
		"url": "https://github.com/cobaltt7"
	},
	"type": "module",
	"main": "./dist/src/index.js",
	"files": [
		"dist",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "rm -rf ./dist && tsc --project tsconfig.json",
		"dev": "tsc --project tsconfig.json --watch",
		"format": "prettier --write \"**\" \"!./dist/\" --ignore-unknown --cache",
		"lint": "eslint",
		"test": "npm run build && node --enable-source-maps --test"
	},
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"dapi-types": "npm:discord-api-types@next",
		"serialize-error": "12.0.0"
	},
	"devDependencies": {
		"@redguy12/prettier-config": "3.4.3",
		"@types/node": "<=18.11",
		"discord.js": "14.21.0",
		"eslint": "9.17.0",
		"eslint-config-cobaltt7": "1.0.0",
		"prettier": "3.5.3",
		"typescript": "5.4.5"
	},
	"peerDependencies": {
		"discord.js": "*"
	},
	"engines": {
		"node": ">=18.11.0",
		"npm": ">=7.0.0"
	},
	"overrides": {
		"@discordjs/builders": "~1.11.0",
		"@discordjs/collection": "1.5.3",
		"@discordjs/formatters": "~0.6.0",
		"@discordjs/ws": "~1.2.1",
		"@sapphire/snowflake": "3.5.3",
		"discord-api-types": "0.38.18",
		"fast-deep-equal": "3.1.3",
		"lodash.snakecase": "4.1.1",
		"tslib": "~2.6.3",
		"undici": "6.21.1",
		"@discordjs/rest": "~2.5.0",
		"@discordjs/util": "~1.1.1"
	}
}
