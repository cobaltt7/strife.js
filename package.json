{
	"name": "strife.js",
	"version": "4.4.0",
	"description": "A Discord bot framework built around TypeScript support and ease of setup.",
	"keywords": [
		"discord",
		"discord-bot",
		"discord.js",
		"typescript"
	],
	"homepage": "https://discord.gg/WaEbDrXKxK",
	"bugs": {
		"url": "https://github.com/cobaltt7/strife.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cobaltt7/strife.js.git"
	},
	"license": "MIT",
	"author": {
		"name": "cobaltt7",
		"url": "https://github.com/cobaltt7"
	},
	"type": "module",
	"main": "./dist/src/index.js",
	"files": [
		"dist",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "rm -rf ./dist && tsc --project tsconfig.json",
		"dev": "tsc --project tsconfig.json --watch",
		"format": "prettier --write \"**\" \"!./dist/\" --ignore-unknown --cache",
		"lint": "eslint",
		"test": "npm run build && cd dist && node --enable-source-maps --test"
	},
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"dapi-types": "npm:discord-api-types@next",
		"serialize-error": "12.0.0"
	},
	"devDependencies": {
		"@redguy12/prettier-config": "3.4.4",
		"@types/node": "<=18.11",
		"discord.js": "14.22.1",
		"eslint": "9.34.0",
		"eslint-config-cobaltt7": "1.3.1",
		"prettier": "3.7.1",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"discord.js": "*"
	},
	"engines": {
		"node": ">=18.11.0",
		"npm": ">=7.0.0"
	}
}
